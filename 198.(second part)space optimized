class Solution {
public:
    int rob(vector<int>& nums) {
        int n=nums.size();
        int prev=nums[0];
        int prev2=0;
        for(int i=0;i<n;i++){
            int t=nums[i];
            if(i>1){
                t+=prev2;
            }
            int nt=0+prev;
            int curi=max(t,nt);
            prev2=prev;
            prev=curi;

        }
        return prev;
        
    }
};
